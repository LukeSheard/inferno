  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/infernojs/inferno.git"
  },
  "scripts": {
    "prebuild": "lerna exec -- rm -rf dist",
    "build": "lerna exec -- node ../../scripts/rollup/build.js",
    "check-version": "node scripts/tasks/check-version.js",
    "postinstall": "lerna bootstrap",
    "lint": "tslint packages/*/src/**/*.ts packages/*/src/**/*.tsx packages/*/src/**/*.js packages/*/src/**/*.jsx",
    "test": "npm run lint && npm run test:unit:cov",
    "test:unit": "jest",
    "test:unit:cov": "jest --coverage",
    "test:unit:publish": "coveralls < coverage/lcov.info",
    "topology": "lerna exec -- pwd",
    "uninstall": "lerna clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.22",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-undefined-to-void": "^6.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concat-stream": "^1.6.0",
    "coveralls": "^2.13.1",
    "jest": "^20.0.3",
    "lerna": "^2.0.0-rc.5",
    "lerna-changelog": "^0.4.0",
    "most": "^1.3.0",
    "most-subject": "^5.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.4.1",
    "sinon": "^2.3.0",
    "ts-jest": "^20.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.3",
    "union-type": "^0.3.3",
    "webpack": "^2.5.1"
  }
